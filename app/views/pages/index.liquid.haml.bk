    :javascript
      $(function () {
          function init() {
              window.addEventListener('scroll', function(e){
                  var distanceY = window.pageYOffset || document.documentElement.scrollTop,
                      shrinkOn = 150,
                      header = document.querySelector("header");
                  if (distanceY > shrinkOn) {
                      $('#top-hea').height(75);
                      document.getElementById("navbar-id").innerHTML = '<img src="{{ 'img/only_logo.png' | theme_image_url }}" alt="" />';   
                  } else {
                      $('#top-hea').height(90);
                      document.getElementById("navbar-id").innerHTML = ' <a >  <img src="{{ 'img/logo_menu.png' | theme_image_url }}" alt="Logo menu" /> </a> <a style="color:white; font-size:14px; padding-right: 30px;"> {{ 'testing_environment' | translate }} </a>';
                  }
              });
          }
          window.onload = init();
          function changeState(el) {
              if (el.readOnly) el.checked=el.readOnly=false;
              else if (!el.checked) el.readOnly=el.indeterminate=true;
          }



          var urlmenu = document.getElementById('multi-languages');
          urlmenu.value = '{{locale}}'
          var url = window.location.pathname;
          var urlsplit = url.split("/").slice(-1)[0];
          
          urlmenu.onchange = function() {
            if (this.options[ this.selectedIndex ].value != '{{locale}}')
              if ((urlsplit== "en") ||(urlsplit== "da")||(urlsplit== ""))
                window.location.href = this.options[ this.selectedIndex ].value;
              else
                window.location.href = url + '/' + this.options[ this.selectedIndex ].value;
          };

      });

