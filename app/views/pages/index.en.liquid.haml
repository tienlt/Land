!!!
%html{:lang => "en-GB"}
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %meta{:charset => "UTF-8"}/
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}/
    %link{:href => "http://gmpg.org/xfn/11", :rel => "profile"}/
    %title Welcome | Product Data Lake
    %link{:href => "//fonts.googleapis.com", :rel => "dns-prefetch"}/
    %link{:href => "//s.w.org", :rel => "dns-prefetch"}/
    %link{:href => "http://soerensen.fr/psi/UK/feed/", :rel => "alternate", :title => "Product Data Lake » Feed", :type => "application/rss+xml"}/
    %link{:href => "http://soerensen.fr/psi/UK/comments/feed/", :rel => "alternate", :title => "Product Data Lake » Comments Feed", :type => "application/rss+xml"}/
    %link#columns-css{:href => "/stylesheets/columns.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/
    %link#contact-form-7-css{:href => "/stylesheets/styles.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/
    %link#cookie-law-info-css{:href => "/stylesheets/cookie-law-info-public.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/
    %link#cookie-law-info-gdpr-css{:href => "/stylesheets/cookie-law-info-gdpr.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/
    %link#meanmenu-css{:href => "/stylesheets/meanmenu.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/
    %link#business-hub-grid-css{:href => "/stylesheets/grid.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/

    %link#owl-carousel-css{:href => "/stylesheets/owl.carousel.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/

    %link#business-hub-style-css{:href => "/stylesheets/application.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/

    %link#business-hub-responsive-css{:href => "/stylesheets/responsive.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/

    %link#font-awesome-css{:href => "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/
    %link#business-hub-fonts-css{:href => "https://fonts.googleapis.com/css?family=Exo+2%3A300%2C400%2C500%2C500i%2C600%2C700%2C900%7CRoboto%3A300%2C400%2C500%2C700%2C900&subset=latin%2Clatin-ext", :media => "all", :rel => "stylesheet", :type => "text/css"}/
    %script{:src => "https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js", :type => "text/javascript"}
  %body.home.page-template-default.page.page-id-40.wp-custom-logo.right-sidebar
    #page.site
      %header#masthead.site-header.header.primary-header.header-logo-text{:role => "banner"}
        .top-bar
          .container
            .row
              .col.col-1-of-2.top-left
                .social-links
                / .social-links
              / .col
              .col.col-1-of-2.top-right
                %form.search-form{:action => "http://soerensen.fr/psi/UK/", :method => "get", :role => "search"}
                  %label
                    %span.screen-reader-text Search for:
                    %input.search-field{:name => "s", :placeholder => "Search …", :type => "search", :value => ""}/
                  %input.search-submit{:type => "submit", :value => "Search"}/
              / .col
            / .row
          / .container
        / .top-bar
        .contact-info-section
          .container
            .row
              .col.col-1-of-4
                .site-branding
                  %a.custom-logo-link{:href => "http://soerensen.fr/psi/UK/", :itemprop => "url", :rel => "home"}
                    %img.custom-logo{:alt => "Product Data Lake", :height => "150", :itemprop => "logo", :sizes => "(max-width: 686px) 100vw, 686px", :src => "http://soerensen.fr/psi/UK/wp-content/uploads/2019/01/OneLineWhiteBold-720x150-tilrettet.png", :srcset => "http://soerensen.fr/psi/UK/wp-content/uploads/2019/01/OneLineWhiteBold-720x150-tilrettet.png 686w, http://soerensen.fr/psi/UK/wp-content/uploads/2019/01/OneLineWhiteBold-720x150-tilrettet-300x66.png 300w", :width => "686"}/
                  %p.site-description Let product data flow easily between trading partners
                / .site-branding
              / .col
              .col.col-3-of-4
                .contact-information-wrapper
                  .contact-information
                    %dl
                      %dt
                        %i.fa.fa-envelope
                      %dd
                        %p
                          %a{:href => "mailto:info@productdatalake.com"} info@productdatalake.com
                          %br/
                          %br/
                          %a{:href => "/da"} DANISH
                / .contact-information
              / .col
        / .contact-info-section
        .full-nav-menu
          .container
            .row
              .col.col-1-of-1
                %nav.main-navigation
                  %ul#menu-main-menu.menu
                    %li.menu-item
                      %a{:href => "http://soerensen.fr/psi/UK/"} Home
                    %li.page_item.page-item-37
                      %a{:href => "http://soerensen.fr/psi/UK/blog/"}
                        %span.dojodigital_toggle_title Blog
                    %li.page_item.page-item-51
                      %a{:href => "http://soerensen.fr/psi/UK/choose-your-business/"}
                        %span.dojodigital_toggle_title Choose your business
                    %li.page_item.page-item-56.page_item_has_children
                      %a{:href => "http://soerensen.fr/psi/UK/information/"}
                        %span.dojodigital_toggle_title Information
                    %li.page_item.page-item-58
                      %a{:href => "http://soerensen.fr/psi/UK/log-on/"}
                        %span.dojodigital_toggle_title Log on
                / .main-navigation
                %form.search-form{:action => "http://soerensen.fr/psi/UK/", :method => "get", :role => "search"}
                  %label
                    %span.screen-reader-text Search for:
                    %input.search-field{:name => "s", :placeholder => "Search …", :type => "search", :value => ""}/
                  %input.search-submit{:type => "submit", :value => "Search"}/
              / .col
            / .row
          / .container
        / .full-nav-menu
      / #masthead
      #main-slider
        .owl-carousel
          .item{:style => "background-image: url(http://soerensen.fr/psi/UK/wp-content/uploads/2019/01/iStock-908478148-slide.jpg); "}
            .caption
              %h2 Welcome
            / .caption
        .scroll-down-wrapper
          .scroll-down
            %a{:href => "#scroll"}
              .scroll-down-middle
              %span.fa.fa-angle-double-down
          / .scroll-down
        / .scroll-down-wrapper
      #primary-home.content-area
        %main#main.site-main{:role => "main"}
          %section.blog-news
            .container
              .row
                .col.col-1-of-1
                  %header.entry-header.heading
                / .col
              / .row
            / .container
          / .blog-news
        / #main
      / #primary
      #content.site-content
        .container
          .row
            #primary.content-area.col.col-2-of-3
              %main#main.site-main{:role => "main"}
                %article#post-40.post-40.page.type-page.status-publish.hentry
                  %header.entry-header
                    %h1.entry-title Welcome
                  / .entry-header
                  .entry-content
                  / .entry-content
                / #post-##
              / #main
            / #primary
          / .row
        / .container
      / #content
      %footer#colophon.site-footer{:role => "contentinfo"}
        #site-generator.site-info
          .container
            .col.col-1-of-1
              .footer-copyright
                #copyright
                  %p Copyright 2016. All rights reserved.
                .go-to-top
                  %a.go-to-top-link{:href => "#"}
                    %i.fa.fa-angle-up
                    = succeed "Back" do
                      %br/
                / .go-to-top
              / .footer-copyright
            / .col
          / .wrapper
        / #site-generator
      / .site-footer
    / #page
    #cookie-law-info-bar
      %span
        This website uses cookies to improve your experience. We'll assume you're ok with this, but you can opt-out if you wish.
        %a#cookie_action_close_header.medium.cli-plugin-button.cli-plugin-main-button.cookie_action_close_header.cli_action_button{"data-cli_action" => "accept", :href => "#"} Accept
        %a#CONSTANT_OPEN_URL.cli-plugin-main-link{:href => "http://soerensen.fr/psi/UK", :target => "_blank"} Read More
    #cookie-law-info-again
      %span#cookie_hdr_showagain Privacy &amp; Cookies Policy
    .cli-modal-backdrop.cli-fade.cli-settings-overlay
    .cli-modal-backdrop.cli-fade.cli-popupbar-overlay
    %script{:src => "/javascripts/application.js", :type => "text/javascript"}
    :javascript
      /* <![CDATA[ */
      var business_hub_script_vars = {"slideSpeed":"3000","autoplayTimeout":"3000"};
      /* ]]> */
